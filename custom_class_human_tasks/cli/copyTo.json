{
  "id": "cli-copy-latest-screenshots",
  "snapshot": "os",
  "instruction": "Open a terminal and copy the two most recent PNG screenshots from ~/Pictures/Screenshots to ~/Documents/traffic_and_weather_data. Create the traffic_and_weather_data folder if doesn't exist.",
  "source": "user",

  "config": [
    {
      "type": "launch",
      "parameters": {
        "command": [
          "bash",
          "-c",
          "echo 'Opening terminal to copy latest screenshots...'"
        ]
      }
    }
  ],

  "trajectory": "trajectories/cli-copy-latest-screenshots",

  "related_apps": [
    "os",
    "terminal"
  ],

  "evaluator": {
    "func": "infeasible"
  },

  "human-ground-truth": {
    "single-action": [
      "`HOTKEY` 'Ctrl+Alt+T'",
      "`WAIT` 500",
      "`TYPING` mkdir -p ~/Documents/traffic_and_weather_data && ls -t ~/Pictures/Screenshots/*.png | head -n 2 | xargs -I{} cp \"{}\" ~/Documents/traffic_and_weather_data/",
      "`PRESS` Enter",
      "`WAIT` 2000",
      "`DONE`"
    ],
    "grouped-action": [
      [
        "`HOTKEY` 'Ctrl+Alt+T'",
        "`WAIT` 500"
      ],
      [
        "`TYPING` mkdir -p ~/Documents/traffic_and_weather_data && ls -t ~/Pictures/Screenshots/*.png | head -n 2 | xargs -I{} cp \"{}\" ~/Documents/traffic_and_weather_data/",
        "`PRESS` Enter",
        "`WAIT` 2000"
      ],
      [
        "`DONE`"
      ]
    ]
  }
}
