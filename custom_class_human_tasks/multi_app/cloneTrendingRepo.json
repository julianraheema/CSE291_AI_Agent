{
  "id": "github-trending-clone-top",
  "snapshot": "chrome",
  "instruction": "Open GitHub Trending, select the top repository, copy its HTTPS clone URL, clone it into ~/Documents/trending_repos, and open it in VS Code.",
  "source": "user",

  "config": [
    {
      "type": "launch",
      "parameters": {
        "command": [
          "google-chrome",
          "--remote-debugging-port=1337",
          "https://github.com/trending"
        ]
      }
    }
  ],

  "trajectory": "trajectories/github-trending-clone-top",

  "related_apps": [
    "chrome",
    "os",
    "terminal",
    "vscode"
  ],

  "evaluator": {
    "func": "infeasible"
  },

  "human-ground-truth": {
    "single-action": [
      "`HOTKEY` 'Ctrl+Alt+T'",
      "`TYPING` google-chrome https://github.com/trending",
      "`PRESS` Enter",
      "`WAIT` 3000",
      "`CLICK` //article[1]//h2/a",
      "`WAIT` 2000",
      "`CLICK` //button[contains(@aria-label, 'Code')]",
      "`WAIT` 500",
      "`CLICK` //button[contains(text(), 'HTTPS')]",
      "`WAIT` 300",
      "`CLICK` //button[@aria-label='Copy']",
      "`WAIT` 500",
      "`HOTKEY` 'Ctrl+Alt+T'",
      "`TYPING` mkdir -p ~/Documents/trending_repos && cd ~/Documents/trending_repos",
      "`PRESS` Enter",
      "`WAIT` 500",
      "`TYPING` git clone $(xclip -o -selection clipboard)",
      "`PRESS` Enter",
      "`WAIT` 5000",
      "`TYPING` cd $(ls -t | head -1)",
      "`PRESS` Enter",
      "`WAIT` 500",
      "`TYPING` code .",
      "`PRESS` Enter",
      "`WAIT` 3000",
      "`DONE`"
    ],

    "grouped-action": [
      [
        "`HOTKEY` 'Ctrl+Alt+T'",
        "`TYPING` google-chrome https://github.com/trending",
        "`PRESS` Enter",
        "`WAIT` 3000"
      ],
      [
        "`CLICK` //article[1]//h2/a",
        "`WAIT` 2000",
        "`CLICK` //button[contains(@aria-label, 'Code')]",
        "`WAIT` 500",
        "`CLICK` //button[contains(text(), 'HTTPS')]",
        "`WAIT` 300",
        "`CLICK` //button[@aria-label='Copy']",
        "`WAIT` 500"
      ],
      [
        "`HOTKEY` 'Ctrl+Alt+T'",
        "`TYPING` mkdir -p ~/Documents/trending_repos && cd ~/Documents/trending_repos",
        "`PRESS` Enter",
        "`WAIT` 500"
      ],
      [
        "`TYPING` git clone $(xclip -o -selection clipboard)",
        "`PRESS` Enter",
        "`WAIT` 5000"
      ],
      [
        "`TYPING` cd $(ls -t | head -1)",
        "`PRESS` Enter",
        "`WAIT` 500",
        "`TYPING` code .",
        "`PRESS` Enter",
        "`WAIT` 3000",
        "`DONE`"
      ]
    ]
  }
}
